{
  "name": "jnstrade",
  "version": "2.28.5",
  "description": "stock trade",
  "main": "main.js",
  "scripts": {
    "start": "set NODE_ENV=dev&& electron .",
    "package": "electron-packager . stocktrade --asar --win --out ./build --arch=x64  --app-version 1.7.9  --overwrite --icon=./app/img/icon/icon.ico",
    "builder": "electron-builder --win --x64"
  },
  "build": {
    "productName": "jnstrade",
    "appId": "com.jns1818.www",
    "copyright": "金牛所",
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://images.huaer168.com/down/win32/"
      }
    ],
    "nsis": {
      "oneClick": false,
      
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./icons/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "金牛所"
    },
    "win": {
      "icon": "icons/icon.ico",
      "requestedExecutionLevel": "highestAvailable",
      "artifactName": "${productName}_setup_${version}.${ext}",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "keywords": [
    "Electron",
    "quick"
  ],
  "author": "jns1818.com",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.6"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "electron-updater": "^4.1.2",
    "ffi": "^2.3.0",
    "node-rsa": "^1.0.6",
    "querystring": "^0.2.0",
    "ref": "^1.3.5",
    "ref-struct": "^1.1.0"
  }
}
